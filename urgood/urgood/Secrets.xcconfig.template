// Secrets Configuration Template
// Copy this file to Secrets.xcconfig and fill in your actual values
// Secrets.xcconfig is gitignored for security

// OpenAI API Key (Required for chat and voice features)
OPENAI_API_KEY = your-openai-api-key-here

// ElevenLabs API Key (Optional - for enhanced voice synthesis)
ELEVENLABS_API_KEY = your-elevenlabs-api-key-here

// RevenueCat API Key (Optional - for subscription management)
REVENUECAT_API_KEY = your-revenuecat-api-key-here

// Push Notification Key ID (Optional)
PUSH_KEY_ID = your-push-key-id-here
